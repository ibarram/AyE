CC = gcc
001 = 01_Ejemplo
002 = 02_Ejemplo
003 = 03_Ejemplo
004 = 04_Ejemplo
005 = 05_Ejemplo

all: $(001) $(002) $(003) $(004) $(005)

$(001): $(001).c
	$(CC) $(001).c -o $(001)

$(002): $(002).c
	$(CC) $(002).c -o $(002)

$(003): $(003).c
	$(CC) $(003).c -o $(003)

$(004): $(004).c
	$(CC) $(004).c -o $(004)

$(005): $(005).c
	$(CC) $(005).c -o $(005)

clean:
	rm -f $(001) $(002) $(003) $(004) $(005)